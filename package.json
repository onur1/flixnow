{
  "name": "@onur1/flixbox",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "lib/server/server/index.js",
  "scripts": {
    "start": "node lib/server/server/index.js",
    "jest": "jest --runInBand",
    "jest-clear-cache": "jest --clearCache",
    "mocha": "mocha -r ts-node/register __tests__/**/*.ts",
    "build": "npm run build-server && npm run webpack-release",
    "build-server": "npm run clean-server && tsc -p tsconfig-server.json",
    "watch-server": "npm run build-server -- -w",
    "clean": "npm run clean-server && npm run clean-app",
    "clean-app": "rimraf lib/app public/bundle.js",
    "clean-server": "rimraf lib/server coverage",
    "lint": "eslint src __tests__",
    "prettier": "prettier --list-different \"./{src,__tests__}/**/*.ts\"",
    "fix-prettier": "prettier --write \"./{src,__tests__}/**/*.ts\"",
    "test": "npm run clean && npm run build-server && npm run lint && npm run prettier && npm run jest-clear-cache && npm run jest",
    "webpack": "npm run clean-app && webpack --config webpack.config.js",
    "webpack-release": "npm run clean-app && NODE_ENV=production npm run webpack",
    "webpack-stats": "npm run clean-app && rimraf stats.json && webpack --json --config webpack.config.js > stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.7.6",
    "@types/react": "^16.14.34",
    "@types/react-dom": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "classnames": "^2.3.2",
    "elm-ts": "^0.6.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "fp-ts": "^2.13.1",
    "fp-ts-contrib": "^0.1.29",
    "history": "^5.3.0",
    "jest": "^29.3.1",
    "mocha": "^10.2.0",
    "monocle-ts": "^2.3.13",
    "prettier": "^2.7.1",
    "prop-types-ts": "^0.7.4",
    "querystring-es3": "^0.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^4.1.0",
    "terser-webpack-plugin": "^5.3.5",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "url": "^0.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@onur1/axios-ts": "^0.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fp-ts-routing": "^0.5.4",
    "hyper-ts": "^0.7.8",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "xache": "^1.1.0"
  }
